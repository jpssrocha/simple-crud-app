<%- include("head.ejs") %>
<body>

    <table class="table table-striped table-sm">
    <thead>
    <tr>
        <th> id </th>
        <th> titulo </th>
        <th> edicao </th>
        <th> descricao </th>
        <th> ideditora </th>
    </tr>
    </thead>
    <tbody>

    <% for (const row of data) { %>
            <% let {id, titulo, edicao, descricao, ideditora} = row; %>
          <tr>
              <th> <%= id %> </th>
              <th> <%= titulo %> </th>
              <th> <%= edicao %> </th>
              <th> <%= descricao %> </th>
              <th> <%= ideditora %> </th>
          </tr>
    <% } %>

    </tbody>
    </table>
 </body>  

 <%- include("foot.ejs") %>
